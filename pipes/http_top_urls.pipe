DESCRIPTION Top URLs requested to the HTTP honeypot.

TOKEN "pipe_read" READ

NODE http_top_urls_node
SQL >
    %
        SELECT count() AS hits, url
        FROM http_requests
        GROUP BY url
        ORDER BY hits DESC
        LIMIT {{ Int32(page_size, 100) }}
        OFFSET {{ Int32(page, 0) * Int32(page_size, 100) }}